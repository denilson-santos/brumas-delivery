<!DOCTYPE html>
<html lang="pt-br">
    <head>
        <meta charset="UTF-8">
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
        <title>Blank Page &mdash; Stisla</title>

        <!-- General CSS Files -->
        <link rel="stylesheet" href="/assets/css/bootstrap/custom.css" />
        <link rel="stylesheet" href="/node_modules/@fortawesome/fontawesome-free/css/all.min.css" />

        <!-- CSS Libraries -->

        <!-- Template CSS -->
        <link rel="stylesheet" href="/assets/admin/css/bootstrap/style.css" />
        <link rel="stylesheet" href="/assets/admin/css/bootstrap/components.css" />
        <link rel="stylesheet" href="/assets/css/general.css" />
        {% block css %}{% endblock css %}
        <link rel="stylesheet" href="/assets/admin/css/templates/header.css" />
        <link rel="stylesheet" href="/assets/admin/css/templates/sidebar.css" />
    </head>

    <body>
        <div id="app">
            <div class="main-wrapper">
                {% include 'admin/templates/header.html.twig' %}
                
                {% include 'admin/templates/sidebar.html.twig' %}
                
                {% block content %}{% endblock content %}
                
                {% include 'admin/templates/footer.html.twig' %}
            </div>
        </div>

        <!-- General JS Scripts -->
        <script src="/node_modules/jquery/dist/jquery.min.js"></script>
        <script type="module" src="/node_modules/popper.js/dist/popper.js"></script>
        <script src="/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
        <script src="/node_modules/nicescroll/dist/jquery.nicescroll.min.js"></script>
        <script type="module" src="/node_modules/moment/dist/moment.js"></script>
        <script src="/assets/admin/js/stisla.js"></script>

        <!-- JS Libraies -->

        <!-- Template JS File -->
        <script src="/assets/admin/js/scripts.js"></script>
        <script src="/assets/admin/js/custom.js"></script>
        {% block js %}{% endblock js %}
        <script src="/assets/admin/js/templates/header.js"></script>
    </body>
</html>